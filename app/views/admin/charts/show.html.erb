<% content_for(:title) do %>
  Dashboard
<% end %>

<header class="main-content__header" role="banner">
  <h1 class="main-content__page-title">
    <%= content_for(:title) %>
  </h1>

  <div>
    <%= link_to t("administrate.actions.back"), :back, class: "button" %>
  </div>
</header>

<section class="main-content__body">
  <%= column_chart data_admin_charts_path(
    resource: params[:resource],
    attribute_to_apply_function: params[:attribute_to_apply_function],
    group_attribute: params[:group_attribute],
    function: params[:function]),
    ytitle: params[:attribute_to_apply_function].humanize,
    xtitle: params[:group_attribute].humanize
  %>
</section>
